{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "types": ["@nuxt/types", "@nuxt/image", "@nuxt/ui"],
    "skipLibCheck": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "target": "esnext",
    "module": "esnext"
  },
  "exclude": [
    "node_modules"
  ]
}
